{
    "name": ":vendor_slug/:package_slug",
    "description": ":package_description",
    "keywords": [
        ":vendor_name",
        "spiral",
        ":package_slug"
    ],
    "homepage": "https://github.com/:vendor_slug/:package_slug",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": "author@domain.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-grpc": "*",
        "grpc/grpc": "^1.42",
        "spiral/boot": "^3.0",
        "spiral/hmvc": "^3.0",
        "spiral/files": "^3.0",
        "spiral/console": "^3.0",
        "spiral/auth": "^3.0",
        "spiral/telemetry": "^3.0",
        "spiral/roadrunner-grpc": "^2.0",
        "spiral/roadrunner-cli": "^2.0",
        "ruvents/spiral-jwt": "^0.1"
    },
    "require-dev": {
        "nette/php-generator": "^3.6",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^9.5.20",
        "spiral/testing": "^2.0",
        "vimeo/psalm": "^4.27"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\Skeleton\\Tests\\App\\": "tests/app",
            "VendorName\\Skeleton\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "psalm": "vendor/bin/psalm --config=psalm.xml ./src"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "spiral": {
            "bootloaders": [
                "VendorName\\Skeleton\\Bootloader\\SkeletonBootloader"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
